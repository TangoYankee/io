{
  "compilerOptions": {
      "baseUrl": "./src",
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "module": "commonjs",
      "target": "es6",
      "jsx": "react"
  },
  "include": ["src/**/*", "index.d.ts"],
  "exclude": ["node_modules", "dist", "coverage", "webpack"],
  "types": ["webpack-env"]
}
